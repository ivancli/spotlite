{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:invigorgroup/invigor-user-management.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:invigorgroup/spotlite-crawler.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:invigorgroup/spotlite-chargify.git"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "almasaeed2010/adminlte": "~2.0",
        "predis/predis": "^1.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "invigor/um": "1.*",
        "maatwebsite/excel": "~2.1.0",
        "invigor/crawler": "1.*",
        "driftyco/ionicons": "2.0.1",
        "mpdf/mpdf": "~5.7.3",
        "facebook/webdriver": "^1.1",
        "invigor/chargify": "dev-master",
        "campaignmonitor/createsend-php": "~5.0.0",
        "geoip2/geoip2": "~2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "phpunit/phpunit-selenium": "> 1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
